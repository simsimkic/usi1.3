<Window x:Class="ZdravoCorp.GUI.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorp.GUI"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="398">
    <Grid>

        <Label Content="Password: " HorizontalAlignment="Left" Height="38" Margin="15,208,0,0" VerticalAlignment="Top" Width="144" RenderTransformOrigin="0.54,0.56"/>
        <Label Content="Username: " HorizontalAlignment="Left" Height="42" Margin="10,94,0,0" VerticalAlignment="Top" Width="134" RenderTransformOrigin="0.5,-0.571"/>
        <TextBox x:Name="UnameBox" HorizontalAlignment="Left" Height="30" Margin="159,97,0,0" VerticalAlignment="Top" Width="167"/>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Left" Height="28" Margin="159,212,0,0" VerticalAlignment="Top" Width="167"/>
        <Label Content="&#x9;&#x9;            Login Window" HorizontalAlignment="Center" Margin="0,39,0,0" VerticalAlignment="Top" Width="426" Height="33" FontWeight="Bold" FontFamily="Segoe UI" FontSize="14" Background="#FFF4E9E9"/>
        <Button Content="Login" Click="Login_Click" HorizontalAlignment="Center" Height="27" Margin="0,324,0,0" VerticalAlignment="Top" Width="166"/>
    </Grid>
</Window>
